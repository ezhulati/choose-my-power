---
import Layout from '../layouts/Layout.astro';
import { TermsOfServicePage } from './_components/TermsOfServicePage';

const title = "Terms of Service | ChooseMyPower.org";
const description = "ChooseMyPower.org terms of service. Terms and conditions for using our electricity comparison and enrollment services.";
---

<Layout title={title} description={description}>
  <TermsOfServicePage client:load />

  <script>
    // Client-side navigation function for React components
    window.navigateToPath = function(path) {
      window.location.href = path;
    };
  </script>
</Layout>