---
import Layout from '../layouts/Layout.astro';
import { TexasPage } from './_components/TexasPage.tsx';

const title = "Texas Electricity Providers & Plans | ChooseMyPower.org";
const description = "Compare Texas electricity providers and plans. Find the best rates, green energy options, and no-deposit plans for your Texas home or business.";
